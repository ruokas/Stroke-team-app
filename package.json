{
  "name": "insulto-komandos-forma",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.8.3",
    "cssnano": "^7.1.1",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "knex": "^3.1.0",
    "lint-staged": "^16.1.6",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5"
  },
  "scripts": {
    "format": "prettier --write \"js/**/*.js\" \"test/**/*.js\"",
    "test": "node --test",
    "build": "node build.js",
    "start": "node server/index.js",
    "prepare": "husky install",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint js test",
    "migrate": "knex --esm migrate:latest --knexfile server/knexfile.js"
  },
  "lint-staged": {
    "{js,test}/**/*.js": [
      "prettier --write",
      "ESLINT_USE_FLAT_CONFIG=false eslint"
    ]
  },
  "dependencies": {
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "nunjucks": "^3.2.4",
    "ws": "^8.18.3"
  }
}
